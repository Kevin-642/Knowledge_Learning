{
    "name": "symfony/skeleton",
    "type": "project",
    "license": "proprietary",
    "description": "A minimal Symfony project recommended to create bare bones applications",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/doctrine-orm": "v4.1.1",
        "api-platform/symfony": "v4.1.1",
        "doctrine/dbal": "^4.2.3",
        "doctrine/doctrine-bundle": "^2.13.2",
        "doctrine/doctrine-fixtures-bundle": "^4.0",
        "doctrine/doctrine-migrations-bundle": "^3.4.1",
        "doctrine/orm": "^3.3.2",
        "easycorp/easyadmin-bundle": "v4.24.5",
        "fakerphp/faker": "v1.24.1",
        "nelmio/cors-bundle": "^2.5",
        "phpdocumentor/reflection-docblock": "^5.6.1",
        "phpstan/phpdoc-parser": "^2.1.0",
        "stripe/stripe-php": "v16.6.0",
        "symfony/asset": "v7.2.0",
        "symfony/asset-mapper": "v7.2.3",
        "symfony/console": "v7.2.1",
        "symfony/debug-bundle": "7.2.*",
        "symfony/doctrine-messenger": "v7.2.3",
        "symfony/dotenv": "v7.2.0",
        "symfony/expression-language": "v7.2.0",
        "symfony/flex": "v2.5.0",
        "symfony/form": "v7.2.4",
        "symfony/framework-bundle": "v7.2.4",
        "symfony/http-client": "v7.2.4",
        "symfony/intl": "v7.2.0",
        "symfony/mailer": "v7.2.3",
        "symfony/mime": "v7.2.4",
        "symfony/monolog-bundle": "v3.10.0",
        "symfony/notifier": "v7.2.0",
        "symfony/process": "v7.2.4",
        "symfony/property-access": "v7.2.3",
        "symfony/property-info": "v7.2.3",
        "symfony/requirements-checker": "*",
        "symfony/runtime": "v7.2.3",
        "symfony/security-bundle": "v7.2.3",
        "symfony/serializer": "v7.2.4",
        "symfony/stimulus-bundle": "^2.23",
        "symfony/string": "v7.2.0",
        "symfony/translation": "v7.2.4",
        "symfony/twig-bundle": "v7.2.0",
        "symfony/ux-turbo": "v2.23.0",
        "symfony/validator": "v7.2.4",
        "symfony/web-link": "v7.2.0",
        "symfony/webpack-encore-bundle": "^2.2",
        "symfony/yaml": "v7.2.3",
        "symfonycasts/reset-password-bundle": "v1.23.1",
        "symfonycasts/verify-email-bundle": "v1.17.3",
        "twig/extra-bundle": "v3.20.0",
        "twig/twig": "v3.20.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd",
            "requirements-checker": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "npm install",
            "npm run build"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.2.*"
        }
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "v8.3.0",
        "friendsofphp/php-cs-fixer": "^3.73",
        "phpunit/phpunit": "^12.0.7",
        "symfony/browser-kit": "v7.2.4",
        "symfony/css-selector": "v7.2.0",
        "symfony/maker-bundle": "v1.62.1",
        "symfony/phpunit-bridge": "v7.2.0",
        "symfony/stopwatch": "v7.2.4",
        "symfony/web-profiler-bundle": "7.2.*"
    }
}
